{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map",
        "dist/**/*.css",
        "dist/assets/**",
        "src/icons/*.ts",
        "src/icon-registry.ts"
      ]
    },
    "analyze": {
      "dependsOn": ["^build"],
      "outputs": ["custom-elements.json"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "format": {
      "dependsOn": []
    },
    "clean": {
      "dependsOn": [],
      "cache": false
    }
  }
}
